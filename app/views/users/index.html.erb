<div ng-controller="UsersCtlr">
  <h1>Users</h1>
  <div>
    <ul class="requests-list">
      <li class="requests-heading">
        <div class="col-md-3">Name</div>
        <div class="col-md-2">Phone</div>
        <div class="col-md-2">Role</div>

      </li>
      <!--li ng-repeat="request in requests | filter:query" class="list-item" -->
      <li class="list-item">
        <div class="col-md-3">{{user.name }}</div>
        <div class="col-md-2">{{user.phone }}</div>
        <div class="col-md-3">{{user.role }}</div>
      </li>
    </ul>
  </div>
</div>
